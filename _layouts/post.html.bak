---
layout: index1
disqus: false
archive: false
---

<!-- Begin Article
================================================== -->


<section class='section blogs' id='blogs'>
  <div class='blog__grid section' id='_posts'>
   {% assign author = site.authors[post.author] %}
 <div class='blog'>
    <span class="read-time" title="Estimated read time">
  <svg id="i-clock" viewBox="0 0 32 32" width="20" height="20" fill="none" stroke="currentcolor" stroke-linecap="round" 
  stroke-linejoin="round" stroke-width="2"><circle cx="16" cy="16" r="14" /><path d="M16 8 L16 16 20 20" /></svg>       

  {% assign words = content | number_of_words %}
  {% if words < 360 %}
    1 min read.
  {% else %}
    {{ words | divided_by:180 }} mins read.
  {% endif %}
</span>
<style>
    svg#i-clock {vertical-align: middle;}
</style>
	
	<h3>{{ page.title }} </h3>
    {{content}}
 </div>
 
  <div id="comments">                          
                {% include disqus.html %} 
            </div>
            

</div> 
    <!-- Pagination -->
    <div style="display:none" class="bottompagination">
      <span class="navigation" role="navigation">
          {% include pagination.html %}
      </span>
  </div>
  <div class='blog__grid2 section' id='_sidebar'>
    {% include sidebar1.html %}
  </div>
  </section>







               
                
               
				
                
		
			
            
           
            
         
            
            
         
           

        

		
           
        


